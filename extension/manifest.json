{
  "manifest_version": 3,
  "name": "GUC downloader",
  "version": "1.0",
  "description": "Downloads files from the GUC website with a readable name",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://cms.guc.edu.eg/apps/student/*"],
      "js": ["content.js"]
    }
  ]
}
